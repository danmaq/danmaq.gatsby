{
    "env": {
        "shared-node-browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "graphql": true,
        "window": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "ecmaVersion": 7,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-param-reassign": 1,
        "no-underscore-dangle": 1,
        "jsx-a11y/anchor-is-valid": 1,
        "react/forbid-prop-types": 1
    }
}